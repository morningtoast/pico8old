__lua__
--Explosion particle lib
--Brian Vaughn, 2016

--[[
	Basic explosion particle library

	In-game use:
	expl_create(x,y, numberOfParticles, optionsTable)
	
	optionsTable
	{
		dur=durationInFrames, --30
		den=startingSizeOfParticle, --0
		decay=rateOfSizeReduction, --.25
		colors=tableOfColors, --{7,10,9}
		smin=minSpeedOfParticles, --1
		smax=maxSpedOfParticles, --1
		grav=maxGravity, --.3
		dir=directionOfForce, --0 (all directions)
		range=distributionAngleAroundDirection --0
	}
	
	
	Add these functions to your system loops:
	expl_update()
	expl_draw()
		
]]

expl_all={}
expl_create=function(x,y, size, options, callback)
	for n=0,size do
		local obj={
			x=x,y=y,
			t=0,
			dur=30,
			den=0,
			decay=.25,
			dia=0,
			colors={7,10,9},
			smin=.25,
			smax=1,
			grav=.3,
			dir=0,
			range=0
		}
		
		if options then
			for k,v in pairs(options) do obj[k] = v end
		end
		
		local c=flr(rnd(#obj.colors))+1
		local sp=rnd(obj.smax-obj.smin)+obj.smin

		if obj.dir>0 then
			local dirh=obj.range/2
			local dira=obj.dir-dirh
			local dirb=obj.dir+dirh
			
			obj.dir=rnd(dirb-dira)+dira
		else
			obj.dir=rnd()	
		end
	
		obj.c=obj.colors[c]
		obj.g=rnd(abs(obj.grav))
		obj.dx=cos(obj.dir)*sp
		obj.dy=sin(obj.dir)*sp
		
		if obj.grav<0 then obj.g*=-1 end

		add(expl_all,obj)
	end
end

expl_update=function()
	for o in all(expl_all) do
		o.dy+=o.g
		o.y+=o.dy
		o.x+=o.dx
		o.t+=1
		o.den-=o.decay
		o.dia=max(o.den,0)

		if o.t>o.dur then del(expl_all, o) end
	end
end

expl_draw=function()
	for e in all(expl_all) do
		circfill(e.x,e.y, e.dia, e.c)
	end
end








-- CART DEMO

--explosion style configurations
--pass these as the third parameter of expl_create() to see different particle patterns
bloodshot={
	dur=15, --last for 15 frames
	den=3, --start with 3-diameter circles
	decay=.4, --reduce circles by .4 each tick
	colors={2,8,8,8,5}, --pick a random color from this list
	smin=1, --minimum speed
	smax=4, --maximum speed
	grav=0, --no gravity, so they don't fall
	dir=1, --force it to the right
	range=.125 --limit range to .125 around direction (.0625 on either side)
}


function _update()
	if btnp(4) or btnp(5) then
		expl_create(64,64, 32)	
	end
	
	expl_update()
end



function _draw()
	cls()
	expl_draw()
	
	print("press <action> to fire",10,10,7)
end






