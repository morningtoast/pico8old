pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
--arena camera demo
--cart 3406

--[[
	BBS Thread: http://www.lexaloffle.com/bbs/?tid=3406
	
	Thanks to scgrn for the code snippet used to make this demo.
	
	This cart demos how to move the camera window when the
	player hits a certain edge distance of the current screen.
	
	This demo includes full X/Y camera motion. Use only the ones
	you need if you're limiting yourself to horizontal or vertical movement.
]]



function _init()
	p={x=10,y=64}
	cam={x=0,y=0}
end

function _update()
	local speed=2 --player speed
	local allowance=50 --distance before camera push. The closer to 64, the closer player gets to the edge
	local playerDiff=5 --extra distance for up/left edges. Adjust as needed
  
	  --left
	if (btn(0)) then
		p.x-=speed
		if (p.x-cam.x<64-allowance-playerDiff) then cam.x-=speed end
	end

	 --right
	if (btn(1)) then
		p.x+=speed
		if (p.x-cam.x>64+allowance) then cam.x+=speed end
	end
	 
	 --up
	if (btn(2)) then
		p.y-=speed
		if (p.y-cam.y<64-allowance-playerDiff) then cam.y-=speed end
	end
	 
	 --down
	if (btn(3)) then
		p.y+=speed
		if (p.y-cam.y>64+allowance) then cam.y+=speed end
	end
 
end



function _draw()
	cls()
	camera(cam.x,cam.y)

	rectfill(0,0,128,128,1)
	circ(64,64,10,7)
	circ(64,64,32,7)
	circ(64,64,64,7)
	circ(64,64,100,7)
	circ(64,64,132,7)
	circ(64,64,164,7)
	line(-100,64,200,64,6)
	line(64,-100,64,200,6)
	
	--player
	circfill(p.x,p.y,3,10)
end


