pico-8 cartridge // http://www.pico-8.com
version 7
__lua__

screen = {}
p = {}

function _init()
	cls()
	screen.update = pcontrols
	p.x = 30
	p.y = 30
	p.spr = 1
	p.flip = false

	rect(0,0,127,127,1)
	rect(2,2,125,125,1)	
	--map(0,0, 0,0, 16,16)	
	spr(1,p.x,p.y)
	
	rx = rnd(120)
	ry = rnd(120)
 
	

end

function _update()
	screen.update()
end

-- up, right, down, left
function pcontrols()

	if (btn(0)) then move(4) end
	if (btn(1)) then move(2) end
	if (btn(2)) then move(1) end
	if (btn(3)) then move(3) end
	
	
end

function move(dr)

	if (dr == 1) then p.y = p.y-1 end
	if (dr == 2) then 
		p.x = p.x+1 
		p.flip = false
	end
	if (dr == 3) then p.y = p.y+1 end
	if (dr == 4) then 
		p.x = p.x-1 
		p.flip = true
	end
	
	
	if (p.x <= 1) then p.x = 1 end
	if (p.y <= 3) then p.y = 3 end
	if (p.y >= (125-8)) then p.y = (125-8) end
	if (p.x >= (125-6)) then p.x = (125-6) end
	
	p.spr = p.spr + 1
	if (p.spr > 2) then p.spr = 1 end
	
	cls()
	spr(4,rx,ry)	
	
	
	rect(0,0,127,127,1)
	rect(2,2,125,125,1)	
	spr(p.spr,p.x,p.y,1,1,p.flip)
	print(solid(p.x,p.y),0,0,7)
 

end

function solid(x, y)
	if (x == rx and y == ry) then
		return true
	else
		return false
	end
 
end
