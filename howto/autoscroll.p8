add(maps.src, {x=31,y=0,w=16,h=16})
add(maps.src, {x=46,y=0,w=16,h=16})

maps.draw=function()
	local screen=0
	
	foreach(level.map, function(id)
		local thismap=maps.src[id]
		
		map(thismap.x,thismap.y, screen*128,0, thismap.w,thismap.h)
		screen+=1
	end)

	
end



function _draw()
 cls()
linex=0
	for n=1,15 do
		line(linex,0,linex,120,12)
		line(0,linex,400,linex,12)
		linex+=12
	end
 -- draw level
 camera(level.cx,0)
 --map(0,0,0,0,level.w/8,level.h/8)
  --map(0,0, 0,0, 16,16)
  --map(0,0, 128,0, 16,16)
  --map(16,0, 0,0, 16,16)
  --maps.draw()

 -- draw player
 spr(16,player.x,player.y)
	
 
	foreach(enemy, function(mob)
		circfill(mob.x,mob.y,5,8)
	end)


 -- debug some stats
 camera()
 print(t,0,0,7)
end

