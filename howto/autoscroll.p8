version 7
__lua__

t=0
function _init()
	mobs={}
 enemy={}
 level={}
 level.w=500 -- width in pixels
 level.h=128 -- height in px
 level.cx=0 -- scrolling offset
 level.map={4,3,2,1}

 player={}
 -- initial position (in px)
 player.xo=50
 player.yo=30
 -- updated player coords
 player.x=player.xo
 player.y=player.yo
 -- player size for collisions
 player.w=8 -- in pixels
 player.h=8
 player.speed=2 -- for movement
 player.ground=false
 
 
  add(mobs,zigzag(250,130,90))
 
 add(mobs,tower(90,130,60))
 add(mobs,tower(90,130,80))
 add(mobs,tower(90,130,100))
 
add(mobs,tower2(90,16,60))
 add(mobs,tower2(90,16,80))
 add(mobs,tower2(90,16,100)) 
 
end


function zigzag(when,x,y)
	local mob={
		spawn={trigger=when, x=x, y=y},
		speed=1,
		route=1,
		lastroute=0,
		timer=0,
		routes={}
	}
	
	add(mob.routes, {ang=.5,t=15,s=2})
	add(mob.routes, {ang=.125,t=30,s=3})
	add(mob.routes, {ang=.5,t=-1,s=2})

	return mob
end

function tower(when,x,y)
	local mob={
		spawn={trigger=when, x=x, y=y},
		route=1,
		timer=0,
		routes={}
	}
	
	add(mob.routes, {ang=.5,t=10,s=2})
	add(mob.routes, {ang=.5,t=90,s=-1})
	add(mob.routes, {ang=.5,t=-1,s=2})

	return mob
end

function tower2(when,x,y)
	local mob={
		spawn={trigger=when, x=x, y=y},
		route=1,
		timer=0,
		routes={}
	}
	
	add(mob.routes, {ang=0,t=10,s=2})
	add(mob.routes, {ang=0,t=90,s=-1})
	add(mob.routes, {ang=0,t=-1,s=4})

	return mob
end


function collisions(o)
 -- compute coordinates of
 -- collision testpoints
 -- (divided by 8, for
 -- compatibility with mget)

 -- left
 local x1p=o.x/8
 -- top
 local y1p=o.y/8
 -- right
 local x2p=(o.x+o.w-1)/8
 -- bottom
 local y2p=(o.y+o.h-1)/8

 -- test collisions

 -- top left
 local x1y1c=fget(mget(x1p,y1p),0)
 -- top right
 local x2y1c=fget(mget(x2p,y1p),0)
 -- bottom left
 local x1y2c=fget(mget(x1p,y2p),0)
 -- bottom right
 local x2y2c=fget(mget(x2p,y2p),0)

 return {tl=x1y1c,tr=x2y1c,bl=x1y2c,br=x2y2c}
end


function _update()
t+=1
 -- scroll map and move player
 if level.cx<level.w-128 then
  level.cx+=1
  player.x+=1
  level.motion=true
 else
	level.motion=false
 end

 -- move the player backwards
 -- if it hits a wall from the
 -- front
 local coll=collisions(player)
 
 if coll.tr or coll.br then
  player.x-=1

 end
 


 -- (for convenience)
 -- if player is slightly out
 -- of screen (left), move it
 -- back until we can see it
 -- entirely
 if player.x-level.cx<0 then
  --player.x+=1
  local coll=collisions(player)
  
  if coll.tr or coll.br then
   -- oops, we moved the player
   -- into a wall. nudge it back
   player.x-=1
  end
 end

 -- per-pixel movement and
 -- collision
 
-- left
 if btn(0) and not(btn(1)) then
  for i=1,player.speed,1 do
   if player.x-level.cx<=0 then
    -- player has reached
    -- screen boundaries,
    -- exit loop
    break
   end
   player.x-=1
   local coll=collisions(player)
   
   if coll.tl or coll.bl then
    -- collision! move player
    -- back to where it didn't
    -- collide
    player.x+=1
    -- exit loop, we won't move
    -- any further
    break
   end
  end
 end
 
 -- right
 if btn(1) and not(btn(0)) then
  for i=1,player.speed,1 do
   if player.x-level.cx>=(127-player.w) then
    break
   end
   player.x+=1
   local coll=collisions(player)
   if coll.tr or coll.br then
    player.x-=1
    break
   end
  end
 end
 
 -- up
 if btn(2) and not(btn(3)) then
	player.ground=false

  for i=1,player.speed,1 do
   if player.y<=0 then
    break
   end
   player.y-=1
   local coll=collisions(player)
   if coll.tl or coll.tr then
    player.y+=1
    break
   end
  end
 end
 
 --down
 if btn(3) or player.ground and not(btn(2)) then
  for i=1,player.speed,1 do
   if player.y>=(127-player.h) then
    break
   end
   player.y+=1
   local coll=collisions(player)
   if coll.bl or coll.br then
	player.ground=true
    if level.motion then player.x-=1 end
	player.y-=1
	
    break
   else 
	player.ground=false
   end
  end
 end
 
 
   if player.x-level.cx<-8 then -- whole sprite is out of bounds, use 0 instead of -8 if you want just immediate out of bounds
   -- player is out of bounds,
   -- game over (reset)
   level.cx=0
   player.x=player.xo
   player.y=player.yo
   player.ground=false
   t=0
   enemy={}
   _init()
   
  end 

  
    foreach(enemy, function(mob)
		local wayp=mob.routes[mob.route]
	
		--mob.ang = atan2(wayp.x-mob.x, wayp.y-mob.y)
		if wayp.s > 0 then
				mob.x+=cos(wayp.ang)*wayp.s
				mob.y+=sin(wayp.ang)*wayp.s
		else
			if wayp.s < 0 then
				if wayp.ang>.25 and wayp.ang<.75 then
					mob.x+=1
				else 
					mob.x+=1
				end
				
			end
		end
		
		mob.timer+=1
		
		if mob.timer==wayp.t then
			mob.route+=1
			mob.timer=0
		end
	end)
  
  foreach(mobs, function(mob)
	if t==mob.spawn.trigger then
		mob.x=level.cx+mob.spawn.x
		mob.y=mob.spawn.y
		
		add(enemy, mob)
	end
  end)
  

  
 
end

