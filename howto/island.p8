pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
-- idle doodle isle
-- by johnicholas

x=0
y=0

--music(0,0,3)

function solid(c,r)
 return c>=0 and r>=0 and
  c<127 and r<127 and
  mget(c,r)==31
end

function _draw()
 cls()
 local r,c=flr(y/8),flr(x/8)
 local t=flr(time())
 camera(x-c*8,y-r*8)
 for i=r,r+17 do
  for j=c,c+17 do
   if (i+j+t)%2==0 then
    spr(1,j*8-c*8,i*8-r*8)
   else
    spr(2,j*8-c*8,i*8-r*8)
   end
   local f=16
   if solid(j,i) then
    f+=1
   end
   if solid(j+1,i) then
    f+=2
   end
   if solid(j,i+1) then
    f+=4
   end
   if solid(j+1,i+1) then
    f+=8
   end
   spr(f,j*8-c*8,i*8-r*8)
  end
 end
 spr(0,60+x-c*8,60+y-r*8)
 -- print(t,60+x-c*8,60+y-r*8)
end

function _update()
 if btn(0) then
  x-=1
 end
 if btn(1) then
  x+=1
 end
 if btn(2) then
  y-=1
 end
 if btn(3) then
  y+=1
 end
end

