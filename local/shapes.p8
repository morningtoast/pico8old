	if btn(0) then ang+=.005 end
	if btn(1) then ang-=.005 end
 
	print(#p2,0,80,10)

end



function rotate(cx, cy, x, y, angle)
    --var radians = (Math.PI / 180) * angle,
        vcos = cos(angle)
        vsin = sin(angle)
        nx = (vcos * (x - cx)) + (vsin * (y - cy)) + cx
        ny = (vcos * (y - cy)) - (vsin * (x - cx)) + cy
    return nx,ny
end

