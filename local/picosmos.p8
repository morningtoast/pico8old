function bprint(s,x,y,cb,cf,srx,sry)
	for i=1,#s do
		xr=flr(rnd(srx))-srx/2
		yr=flr(rnd(sry))-sry/2
		ss=sub(s,i,i)
		for xx=-1,1 do
			for yy=-1,1 do
				print(ss, xr+xx+x+4*i, yr+yy+y, cb)
			end
		end
		print(ss, xr+x+4*i, yr+y, cf)
	end
end

