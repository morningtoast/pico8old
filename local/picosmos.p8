	 t.x = rnd(128)-64
	 t.y = rnd(128)-64
	 sp = 0.4
	 t.xs= rnd(sp)-sp/2
	 t.ys= rnd(sp)-sp/2
  --t.xs=0
  --t.ys=0
	 add(w,t) 
