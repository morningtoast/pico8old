function makemenu()
 rectfill(0,0,127,127,0)
 print("[picosmos]",0,40,15)

	circfill(60,43,10,11)
	circfill(80,68,15,15)
	circfill(105,99,20,8)


	if btn(4) then
		initgame()
		show.stage="game"
	end
end




