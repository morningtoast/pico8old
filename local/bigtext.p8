--[[
14th of june 2016

left and right controls the scrolling
up and down controls the wave frequency

check http://www.lexaloffle.com/bbs/?uid=12981
for updates
]]

function scan_text(text)
  cls()
  scan={}
  print(text,0,0,1)
  for x=0,(#text)*4 do
    scan[x]={}
    for y=0,6 do
      scan[x][y]=pget(x,y)
    end
  end
  cls()
  return scan
end

function put_sintext(text,x,y,effect)
  local i,j
  for i=0,#text do
    for j=0,6 do
      a=sin(angle)*6
      if text[i][j]==1 then
        if (effect=='rect') rectfill(i*3+x, j*3+y+a, i*3+x+3, j*3+y+3+a, (i+j)%7+8)
        if (effect=='circ') circ(i*4+x, j*4+y+a, 1, (i+j)%3+12)
        if (effect=='pixl') pset(i*2+x, j*2+y+a,    (i+j)%3+8)
        --if (effect=='big')  rectfill(i*8+x, j*8+y, i*8+x+7, j*8+y+7, 1)
		if (effect=='big')  rectfill(i*5+x, j*5+y, i*5+x+7, j*5+y+7, 1)
        if (effect=='col-only') pset(i+x, j+y,    (i+j)%3+12)
      end
    end
    angle+=dangle
    if (angle>1) angle=0
  end
end

function _init()
  txt0=scan_text('1000')
  txt1=scan_text("500")
  txt2=scan_text("any t3xt")
  txt3=scan_text ("no sprite")
  txt4=scan_text ("dangle :")
  txt5=scan_text ("speed :")
  
  angle=0 -- todo get rid of these global variables and store them into the sintext
  dangle=0.024
  dx=0
  speed=0
end

function _draw()
  camera(dx,0)
  cls()
							
  put_sintext(txt0,1,0,'big')
  put_sintext(txt1,1,30,'big')
  --[[put_sintext(txt1,33,10,'circ')
  put_sintext(txt2,20,52,'rect')
  put_sintext(txt3,27,92,'pixl')
  put_sintext(txt4,dx,120,'col-only')
  put_sintext(txt5,dx+70,120,'col-only')
  ]]
  --print(dangle,dx+35,120,7)
  --print(speed,dx+100,120,7)
							
							circ(40,40,8,10)
end
 
function _update()
  dx+=speed
  if (abs(speed)<0.1) speed=0
  if (dx>127)  dx=-127
  if (dx<-127) dx=127
  if (btn(0)) speed+=.5
  if (btn(1)) speed-=.5
  if (btn(2)) dangle+=.0001
  if (btn(3)) dangle-=.0001
end


